initialize
watch Reset
watch _Reset
watch var_x.v.t
watch var_x.v.f
watch var_y.v.t
watch var_y.v.f
set_principal var_x.v.t
set_principal var_x.v.f
set_principal var_y.v.t
set_principal var_y.v.f

mode reset
set Reset 1
set _Reset 0
set var_x.v.t 0
set var_x.v.f 0
set var_y.v.t 0
set var_y.v.f 0
cycle
set Reset 0
set _Reset 1
cycle
mode run
set var_x.v.t 0
set var_x.v.f 1
set var_y.v.t 0
set var_y.v.f 1
cycle

assert var_x.v.t 0
assert var_x.v.f 1
assert var_y.v.t 0
assert var_y.v.f 1
set var_x.v.t 0
set var_x.v.f 1
set var_y.v.t 1
set var_y.v.f 0
cycle

assert var_x.v.t 0
assert var_x.v.f 1
assert var_y.v.t 1
assert var_y.v.f 0
set var_x.v.t 1
set var_x.v.f 0
set var_y.v.t 1
set var_y.v.f 0
cycle

assert var_x.v.t 1
assert var_x.v.f 0
assert var_y.v.t 1
assert var_y.v.f 0
