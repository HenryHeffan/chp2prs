initialize
random
watch Reset
watch _Reset
watch GND
watch t.go.r
watch t.var_x.v.t
watch t.var_x.v.f
watch t.var_y.v.t
watch t.var_y.v.f
watch t.go.a
set_principal t.go.r
set_principal t.var_x.v.t
set_principal t.var_x.v.f
set_principal t.var_y.v.t
set_principal t.var_y.v.f
set_principal t.go.a

mode reset
set Reset 1
set _Reset 0
set GND 0
set t.go.r 0
set t.go.a 0
cycle
set Reset 0
set _Reset 1
cycle
mode run
set t.go.r 0
set t.go.a 0
cycle

fanin t.c_5.a
assert t.go.r 0
assert t.var_x.v.t 0
assert t.var_x.v.f 1
assert t.var_y.v.t 0
assert t.var_y.v.f 1
