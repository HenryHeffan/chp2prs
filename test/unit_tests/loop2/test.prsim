initialize
watch Reset
watch _Reset
watch GND
watch t.go.r
watch t.var_x.v.t
watch t.var_x.v.f
watch t.var_y.v.t
watch t.var_y.v.f
watch t.var_z.v.t
watch t.var_z.v.f
watch t.var_w.v.t
watch t.var_w.v.f
watch t.var_sel0[0].v.t
watch t.var_sel0[0].v.f
watch t.var_sel0[1].v.t
watch t.var_sel0[1].v.f
watch t.var_sel0[2].v.t
watch t.var_sel0[2].v.f
watch t.var_sel0[3].v.t
watch t.var_sel0[3].v.f
watch t.go.a
set_principal t.go.r
set_principal t.var_x.v.t
set_principal t.var_x.v.f
set_principal t.var_y.v.t
set_principal t.var_y.v.f
set_principal t.var_z.v.t
set_principal t.var_z.v.f
set_principal t.var_w.v.t
