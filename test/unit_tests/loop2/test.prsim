set_principal t.var_sel0[0].v.t
set_principal t.var_sel0[0].v.f
set_principal t.var_sel0[1].v.t
set_principal t.var_sel0[1].v.f
set_principal t.var_sel0[2].v.t
set_principal t.var_sel0[2].v.f
set_principal t.var_sel0[3].v.t
set_principal t.var_sel0[3].v.f
set_principal t.go.a

mode reset
set Reset 1
set _Reset 0
set t.go.r 0
set t.go.a 0
set GND 0
cycle
set Reset 0
set _Reset 1
cycle
mode run
set t.go.r 0
set t.go.a 0
cycle

assert t.go.r 0
assert t.var_x.v.t 0
assert t.var_x.v.f 1
assert t.var_y.v.t 0
assert t.var_y.v.f 1
assert t.var_z.v.t 0
assert t.var_z.v.f 1
assert t.var_w.v.t 0
assert t.var_w.v.f 1
assert t.var_sel0[0].v.t 0
assert t.var_sel0[0].v.f 1
assert t.var_sel0[1].v.t 0
assert t.var_sel0[1].v.f 1
assert t.var_sel0[2].v.t 0
assert t.var_sel0[2].v.f 1
assert t.var_sel0[3].v.t 0
assert t.var_sel0[3].v.f 1
assert t.go.a 0
set t.go.r 1
cycle

assert t.go.r 1
assert t.var_x.v.t 0
assert t.var_x.v.f 1
assert t.var_y.v.t 0
assert t.var_y.v.f 1
assert t.var_z.v.t 0
assert t.var_z.v.f 1
assert t.var_w.v.t 0
assert t.var_w.v.f 1
assert t.var_sel0[0].v.t 1
assert t.var_sel0[0].v.f 0
assert t.var_sel0[1].v.t 0
assert t.var_sel0[1].v.f 1
assert t.var_sel0[2].v.t 0
assert t.var_sel0[2].v.f 1
