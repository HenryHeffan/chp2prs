initialize
watch Reset
watch _Reset
watch c_0.r
watch c_0.a
set_principal c_0.r
set_principal c_0.a

mode reset
set Reset 1
set _Reset 0
set c_0.r 0
set c_0.a 0
cycle
set Reset 0
set _Reset 1
cycle
mode run
set c_0.r 0
set c_0.a 0
cycle

assert c_0.r 0
assert c_0.a 0
set c_0.r 1
set c_0.a 0
cycle

assert c_0.r 1
assert c_0.a 0
set c_0.r 1
set c_0.a 1
cycle

assert c_0.r 1
assert c_0.a 1
set c_0.r 0
set c_0.a 1
cycle

assert c_0.r 0
assert c_0.a 1
set c_0.a 0
cycle

assert c_0.a 0
